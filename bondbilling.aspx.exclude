        function SetRate() {
            var clientID = '<%= rdbRateCategory.ClientID %>';

            for (i = 0; i < '<%= rdbRateCategory.Items.Count %>'; i++) {
                if (document.getElementById(clientID + "_" + i.toString()).checked) {
                    document.getElementById("<%=txtBondRate.ClientID%>").value = document.getElementById(clientID + "_" + i.toString()).value;
                    //there are 2 different commissions depending on the rate selected
                    if (i == 0 || i == 1) {
                        document.getElementById("<%=txtCommission.ClientID%>").value = document.getElementById("<%=hdnAccountCommission.ClientID%>").value;
                    }
                    else {
                        document.getElementById("<%=txtCommission.ClientID%>").value = document.getElementById("<%=hdnIndividualCommission.ClientID%>").value;
                    }
                    break;
                }
            }
        }

        function addYear() {
            if ((document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_radioTermlist_0").checked == true) || (document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_radioTermlist_1").checked == true) || (document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_radioTermlist_2").checked == true)) {
                if (document.getElementById("<%=txtEffectivedt.ClientID%>").value == "") {
                    document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_radioTermlist_0").checked = false
                    document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_radioTermlist_1").checked = false
                    document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_radioTermlist_2").checked = false
                    alert('Please select effective date');
                    return false;
                }
                else {
                    var fDate = new Date(document.getElementById("<%=txtEffectivedt.ClientID%>").value);
                    fDate.setDate(fDate.getDate() + 365);
                    var MM = fDate.getMonth() + 1;
                    var DD = fDate.getDate();
                    var YY = fDate.getFullYear();
                    if (MM < 10) MM = "0" + MM;
                    if (DD < 10) DD = "0" + DD;
                    document.getElementById("<%=txtExpiryDt.ClientID%>").value = MM + "/" + DD + "/" + YY;
                }
            }
        }

