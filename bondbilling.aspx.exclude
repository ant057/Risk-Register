        function FrontingSplit() {
            var fc = document.getElementById("<%=txtFBrokerCommission.ClientID%>").value;
            if (fc != "" && fc != "0") {
                var splitpercent = 0;
                var splitcomm = removeCurrencyAttributes(document.getElementById("<%=txtFBrokerCommission.ClientID%>").value);
                var comm = removeCurrencyAttributes(document.getElementById("<%=txtCommissionPremium.ClientID%>").value);
                comm = comm
                splitpercent = parseFloat(splitcomm) / parseFloat(comm);
                splitpercent = splitpercent * 100;
                document.getElementById("<%=txtFCommSplit.ClientID%>").value = roundNumber(splitpercent, 2) + "%";
            } else { document.getElementById("<%=txtFCommSplit.ClientID%>").value = "0 %"; }
        }

