        function CalculateAdjustedPremium() {
            var bondamount = document.getElementById("<%=txtbondAmount.ClientID%>").value;
            bondamount = bondamount.replace(',', '');
            bondamount = bondamount.replace(',', '');
            bondamount = bondamount.replace(',', '');
            bondamount = bondamount.replace(',', '');
            bondamount = bondamount.replace(',', '');
            bondamount = bondamount.replace(',', '');
            bondamount = bondamount.replace(',', '');
            bondamount = bondamount.replace(',', '');
            var adjPremiumrate = document.getElementById("<%=txtAdJustedRate.ClientID%>").value;
            adjPremiumrate = adjPremiumrate.replace(',', '');
            adjPremiumrate = adjPremiumrate.replace(',', '');
            adjPremiumrate = adjPremiumrate.replace(',', '');
            adjPremiumrate = adjPremiumrate.replace(',', '');

            if (adjPremiumrate == "") {
                alert('Please enter adjusted rate');
                document.getElementById("<%=txtAdJustedRate.ClientID%>").focus();
                return false;
            }

            var PremiumCommission = document.getElementById("<%=txtCommission.ClientID%>").value;
            document.getElementById("<%=txtAdjustedPremium.ClientID%>").value = "";
            document.getElementById("<%=txtAdjustedPremium.ClientID%>").value = roundNumber(((adjPremiumrate / 100) * bondamount), 2);

            if (PremiumCommission != 0) {
                var gwPremium = document.getElementById("<%=txtAdjustedPremium.ClientID%>").value;
                document.getElementById("<%=txtadjustedCommission.ClientID%>").value = "";
                document.getElementById("<%=txtadjustedCommission.ClientID%>").value = roundNumber(gwPremium * (PremiumCommission / 100), 2);
            }
            else {
                document.getElementById("<%=txtadjustedCommission.ClientID%>").value = "";
                document.getElementById("<%=txtadjustedCommission.ClientID%>").value = 0;
            }
        }

