        function CalculateCommission() {
            var GWPremium = removeCurrencyAttributes(document.getElementById("<%=txtGWPremium.ClientID%>").value);
            var CommissionPercent = removeCurrencyAttributes(document.getElementById("<%=txtCommission.ClientID%>").value);
            var BusinessType = document.getElementById("<%=hdnBusinessType.ClientID%>").value;

            if (BusinessType == "1") {
                var CommissionPremium = parseFloat(GWPremium) * parseFloat(CommissionPercent / 100);
                document.getElementById("<%=txtCommissionPremium.ClientID%>").value = CommissionPremium;
                document.getElementById("<%=txtGWPremium.ClientID%>").value = formatNumber(document.getElementById("<%=txtGWPremium.ClientID%>").value, '#,###.00');
                document.getElementById("<%=txtCommissionPremium.ClientID%>").value = formatNumber(document.getElementById("<%=txtCommissionPremium.ClientID%>").value, '#,###.00');
            }
        }

        function CalculateEndorsementPremium() {
            var BusinessType = document.getElementById("<%=hdnBusinessType.ClientID%>").value;

            //Check to see if the entered values are valid dollar amounts
            if (isMoney(document.getElementById("<%=txtEndorsedPremium.ClientID%>").value)) {
                //Calculate the new Total Gross Written Premium
                var GWPPremium = removeCurrencyAttributes(document.getElementById("<%=txtGWPremium.ClientID%>").value);
                var endorsePremium = removeCurrencyAttributes(document.getElementById("<%=txtSystemEndorsementAmount.ClientID%>").value);
                var changedEndorsedPremium = removeCurrencyAttributes(document.getElementById("<%=txtEndorsedPremium.ClientID%>").value)

                //Calculate the new Total Premium
                var calculatedNewPremium = parseFloat(GWPPremium) - parseFloat(endorsePremium) + parseFloat(changedEndorsedPremium);
                document.getElementById("<%=txtGWPremium.ClientID%>").value = roundNumber(calculatedNewPremium, 2);
                document.getElementById("<%=txtGWPremium.ClientID%>").value = formatNumber(document.getElementById("<%=txtGWPremium.ClientID%>").value, '#,###.00');

                if (BusinessType == "1") {
                    //Calculate the Total Commission
                    var NewGWPremium = removeCurrencyAttributes(document.getElementById("<%=txtGWPremium.ClientID%>").value);
                    var CommissionPercent = removeCurrencyAttributes(document.getElementById("<%=txtCommission.ClientID%>").value);
                    var CommissionPremium = parseFloat(NewGWPremium) * parseFloat(CommissionPercent / 100);
                    document.getElementById("<%=txtCommissionPremium.ClientID%>").value = CommissionPremium;
                    document.getElementById("<%=txtCommissionPremium.ClientID%>").value = formatNumber(document.getElementById("<%=txtCommissionPremium.ClientID%>").value, '#,###.00');

                    //Calculate the Endorsement Commission    
                    var EndorsementCommissionPremium = parseFloat(changedEndorsedPremium) * parseFloat(CommissionPercent / 100);
                    document.getElementById("<%=txtEndorsementCommissionAmount.ClientID%>").value = EndorsementCommissionPremium;
                    document.getElementById("<%=txtEndorsementCommissionAmount.ClientID%>").value = formatNumber(document.getElementById("<%=txtEndorsementCommissionAmount.ClientID%>").value, '#,###.00');
                }
                else {
                    //need to loop through the contract table to recalc contract commission
                }
                //Format the Endorsement Premium
                document.getElementById("<%=txtEndorsedPremium.ClientID%>").value = formatNumber(document.getElementById("<%=txtEndorsedPremium.ClientID%>").value, '#,###.00');
            }
            else {
                alert('Please enter a valid currency value in the Endorsement Premium field.');
            }
        }

        //used for the manual changing of a contract commission
        function CalculateContractCommissionChange() {
            var BusinessType = document.getElementById("<%=hdnBusinessType.ClientID%>").value;
            if (BusinessType != "1") {
                var OriginalCommission = parseFloat(removeCurrencyAttributes(document.getElementById("<%=txtCommissionPremium.ClientID%>").value));
                var EndorsementCommissionPremium = parseFloat(removeCurrencyAttributes(document.getElementById("<%=txtEndorsementCommissionAmount.ClientID%>").value));
                var NewCommissionPremium = OriginalCommission + EndorsementCommissionPremium;
                document.getElementById("<%=txtCommissionPremium.ClientID%>").value = NewCommissionPremium;
                document.getElementById("<%=txtCommissionPremium.ClientID%>").value = formatNumber(document.getElementById("<%=txtCommissionPremium.ClientID%>").value, '#,###.##');
                //                alert(OriginalCommission);
                //                alert(EndorsementCommissionPremium);
                //                alert(NewCommissionPremium);
                //alert(formatNumber(document.getElementById("<%=txtCommissionPremium.ClientID%>").value, '#,###.##'));
            }
        }

